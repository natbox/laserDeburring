FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.copper 0
);

regions
(
    boxToCell
    {
        box (0 0 -1) (0.292 0.398 0.010);
        fieldValues
        (
            volScalarFieldValue alpha.copper 1
        );
    }
);


