FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.zn 0
);

regions
(
    boxToCell
    {
        box (-0.9 -0.9 0) (0.04 0.04 0.01);
        fieldValues
        (
            volScalarFieldValue alpha.zn 1
        );
    }
);


